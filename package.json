{
  "name": "vite-csp",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "p:build": "turbo build --filter=vite-plugin-hash-csp",
    "p:test": "turbo test --filter=vite-plugin-hash-csp",
    "dev": "turbo dev",
    "preview": "turbo preview",
    "r:dev": "turbo dev --filter=react-app",
    "r:build": "turbo build --filter=react-app",
    "r:preview": "turbo preview --filter=react-app",
    "mui:dev": "turbo dev --filter=mui",
    "mui:build": "turbo build --filter=mui",
    "mui:preview": "turbo preview --filter=mui",
    "em:dev": "turbo dev --filter=emotion",
    "em:build": "turbo build --filter=emotion",
    "em:preview": "turbo preview --filter=emotion",
    "tailwind:dev": "turbo dev --filter=tailwind",
    "tailwind:build": "turbo build --filter=tailwind",
    "tailwind:preview": "turbo preview --filter=tailwind",
    "sass:dev": "turbo dev --filter=sass",
    "sass:build": "turbo build --filter=sass",
    "sass:preview": "turbo preview --filter=sass",
    "less:dev": "turbo dev --filter=less",
    "less:build": "turbo build --filter=less",
    "less:preview": "turbo preview --filter=less",
    "stylus:dev": "turbo dev --filter=stylus",
    "stylus:build": "turbo build --filter=stylus",
    "stylus:preview": "turbo preview --filter=stylus",
    "docs:dev": "turbo dev --filter=docs",
    "docs:build": "turbo build --filter=docs",
    "test": "pnpm exec playwright test",
    "test:ui": "pnpm exec playwright test --ui",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "bash scripts/release.sh vite-plugin-hash-csp"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@repo/eslint-config": "workspace:^",
    "@repo/typescript-config": "workspace:^",
    "@types/node": "^20.14.2",
    "prettier": "^3.1.1",
    "turbo": "^2.0.0",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.6",
  "version": "0.0.2"
}
